<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".main_activity"
    >


    <RelativeLayout android:id="@+id/content_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout android:id="@+id/page_student_ek1"
            android:layout_marginTop="0dp"
            android:layout_marginStart="0dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <RelativeLayout
                android:id="@+id/student_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/logandset">

                <ImageView
                    android:id="@+id/backbutton"
                    android:layout_width="wrap_content"
                    android:layout_height="34dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="357dp"
                    android:background="@drawable/backarrow" />

                <TextView
                    android:id="@+id/reg"
                    android:layout_width="268dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/backbutton"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="67dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginEnd="71dp"
                    android:fontFamily="@font/inter_semibold"
                    android:gravity="center_horizontal|top"
                    android:text="Register"
                    android:textAppearance="@style/login" />

                <TextView
                    android:id="@+id/regdet"
                    android:layout_width="268dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/reg"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="67dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="71dp"
                    android:fontFamily="@font/inter"
                    android:gravity="center_horizontal|top"
                    android:text="enter your personal data"
                    android:textAppearance="@style/welcome" />

                <TextView
                    android:id="@+id/email_addre"
                    android:layout_width="266dp"
                    android:layout_height="28dp"
                    android:layout_below="@+id/regdet"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="-22dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="167dp"
                    android:fontFamily="@font/inter_semibold"
                    android:gravity="center_horizontal|top"
                    android:text="Email Address"
                    android:textAppearance="@style/email_addre" />

                <EditText
                    android:id="@+id/emailText"
                    android:layout_width="367dp"
                    android:layout_height="70dp"
                    android:layout_below="@+id/email_addre"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/text"
                    android:ems="10"
                    android:inputType="textEmailAddress"
                    android:paddingLeft="25dp"
                    android:maxLines="1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/fname"
                    android:layout_width="266dp"
                    android:layout_height="28dp"
                    android:layout_below="@+id/emailText"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="-20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="203dp"
                    android:fontFamily="@font/inter_semibold"
                    android:gravity="center_horizontal|top"
                    android:text="Full Name"
                    android:textAppearance="@style/email_addre" />

                <EditText
                    android:id="@+id/Fname"
                    android:layout_width="367dp"
                    android:layout_height="70dp"
                    android:layout_below="@+id/fname"
                    android:layout_centerHorizontal="true"
                    android:layout_marginStart="3dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/text"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqestuvwxyz "
                    android:ems="10"
                    android:inputType="text"
                    android:paddingLeft="25dp"
                    android:maxLines="1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/lname"
                    android:layout_width="266dp"
                    android:layout_height="32dp"
                    android:layout_below="@+id/Fname"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="197dp"
                    android:fontFamily="@font/inter_semibold"
                    android:gravity="center_horizontal|top"
                    android:text="Occupation"
                    android:textAppearance="@style/email_addre" />

                <Spinner
                    android:id="@+id/hal"
                    android:layout_width="367dp"
                    android:layout_height="70dp"
                    android:layout_below="@+id/lname"
                    android:layout_marginTop="5dp"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/text"
                    android:fontFamily="@font/inter_semibold"
                    android:paddingLeft="25dp"
                    android:spinnerMode="dropdown"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/phone"
                    android:layout_width="266dp"
                    android:layout_height="28dp"
                    android:layout_below="@+id/hal"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="-2dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginEnd="277dp"
                    android:fontFamily="@font/inter_semibold"
                    android:gravity="center_horizontal|top"
                    android:text="ID"
                    android:textAppearance="@style/email_addre" />

                <EditText
                    android:id="@+id/phonenum"
                    android:layout_width="367dp"
                    android:layout_height="70dp"
                    android:layout_below="@+id/phone"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/text"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:inputType="numberDecimal"
                    android:paddingLeft="25dp"
                    android:maxLines="1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/logbutton"
                    android:layout_width="367dp"
                    android:layout_height="70dp"
                    android:layout_below="@+id/phonenum"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#8C51FE"
                    android:fontFamily="@font/inter_semibold"
                    android:text="Register"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="25sp"
                    app:cornerRadius="50dp" />


            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>